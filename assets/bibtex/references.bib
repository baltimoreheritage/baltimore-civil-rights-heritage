@phdthesis{Anderson-FreeBaltimoreColoredOrchestra1994,
  address = {United States -- Alabama},
  type = {D.{{M}}.{{A}}.},
  title = {The {{Baltimore Colored Orchestra}} and the {{City Colored Chorus}}},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {The contributions of African-American performers to music making in the United States are usually conceived in terms of various popular styles such as jazz, spirituals and gospel music. The present study will demonstrate that their contribution, even in the days of segregation, was still more wide-ranging, encompassing Western art music as well. As proof of this thesis, this study will document the activities of the Baltimore City Colored Orchestra and City Colored Chorus. It focuses on the involvement of African Americans in the 1930s as a segregated group in the United States in the area of classical performance.
The orchestra was established in 1929, and the chorus two years later. Both organizations were sponsored by the Baltimore City Municipal Department. They were the only minority municipally sponsored orchestra and chorus in the United States during the 1930s.
This research reconstructs the experiences and reactions of the white and black communities toward the orchestra and chorus. The details of the organizations' development are presented, along with narrative descriptions of the concerts, radio broadcasts, repertory and critics' reviews. Biographical information concerning the conductors, soloists and other pertinent persons is supplied. The study also reviews other cultural activities in the African-American community: the youth symphony, school performances, and church musical activities. The involvement of the Federation of Musicians Union is also discussed.
The purposes of the Baltimore City Colored Orchestra and City Colored Chorus were to create and educate appreciative audiences of all ages through their exposure to classical concerts, to develop the performance capabilities of the musicians by learning and playing the classical repertoire, and to feature new talent.
This study will supply some information regarding African Americans in American music history. Hence, the existence of the orchestra and chorus is a means of understanding some of the positive and negative influences on Baltimore's African American community and its musical heritage.},
  language = {English},
  school = {The University of Alabama},
  author = {{Anderson-Free}, Corine F.},
  year = {1994},
  keywords = {African american,Maryland,Social sciences,Communication and the arts},
  file = {/Users/elipousson/Dropbox/Zotero/storage/X32PFXX4/Anderson-Free_1994_The Baltimore Colored Orchestra and the City Colored Chorus.pdf}
}

@phdthesis{AllenCautiouslycapitalisticBlack2013,
  address = {United States -- Maryland},
  type = {Ph.{{D}}.},
  title = {Cautiously Capitalistic: {{Black}} Economic Agency at the {{Savings Bank}} of {{Baltimore}}, 1850\textendash{}1900},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Cautiously Capitalistic},
  abstract = {How much did racial oppression and other social barriers affect African-American wealth accumulation during antebellum and postbellum America? Were African Americans totally regulated to the economic dregs of society? This study uses banking records to chart and understand economic behaviors of African Americans in Baltimore before and after the Civil War. By extracting the debits and credits from the deposit ledgers of the Savings Bank of Baltimore\textemdash{}a white savings bank\textemdash{}of African Americans and triangulating these ledgers with census and land records, and newspapers more details concerning their economic lives emerge. Although many could not read and write, black savers proactively patronized this savings bank, eventually learning to use the interest and extra dividends they earned to accomplish individual and community goals, such as purchasing property and funding benevolence organizations. The Bank's method of disseminating interest and extra dividends rewarded black savers with large accounts that left their monies in the bank over long periods of time. Many black families came together, using a form of communal or collective capitalism, to make large deposits, benefiting from the policy. Although, other factors determined the success of savers utilizing this policy, such as occupation, self-discipline, and family structure, this policy allowed black people at different income levels to circumvent some of the socio-economic factors that limited wealth and security for individuals. African Americans, through their savings accounts accumulated more wealth than we associate with the antebellum and postbellum era.},
  language = {English},
  school = {Morgan State University},
  author = {Allen, Marcus Anthony},
  year = {2013},
  keywords = {1870-1905,Social sciences,Banking,Black barbers,Black capitalism,Free blacks,Property,Savings accounts},
  file = {/Users/elipousson/Dropbox/Zotero/storage/NBSTE57N/Allen_2013_Cautiously capitalistic.pdf}
}

@phdthesis{JohnsonBlackliveswhite2002,
  address = {United States -- New York},
  type = {Ph.{{D}}.},
  title = {Black Lives and White Minds: {{Race}} and Perception in Antebellum {{Baltimore}}},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Black Lives and White Minds},
  abstract = {This dissertation has two interrelated goals. First, I examine the perception of Baltimore's free African Americans by the city's white population. I concentrate upon the infection of a specific locale\textemdash{}Baltimore\textemdash{}with white supremacist language. White Baltimoreans adhered to a complex and dense mass of ideas and images based on the assumption of black inferiority. My work combines two strands of historiography that are generally treated separately. I compliment African American historiography and studies of white racial ideas by treating both in a local study. I find a vast gulf between white Baltimorean's imagery of African Americans and the way that the free black population of the city conducted their lives. White Baltimoreans perceived free blacks as a despicable group whose natural condition was enslavement. They imagined free black people as promiscuous, irresponsible, undisciplined, and incapable of any but the most fleeting emotional ties. Baltimore whites did not see that the city's free blacks contradicted degrading ideas projected upon them by overcoming difficult conditions to live autonomously from whites in a lawful, dignified, and responsible manner.
I concentrate upon the infection of a specific locale-Baltimore-with white supremacist language. White Baltimoreans adhered to a complex and dense mass of ideas and images based on the assumption of black inferiority. I focus upon white perceptions of free African Americans through a variety of lenses. Racial ideology emanated from the pulpit, press, scientific authorities, and the public square to create consistent imagery of African Americans. I argue that free African Americans in Baltimore put the lie to white supremacist ideology by transcending abysmal rural conditions to live dignified, largely independent lives.},
  language = {English},
  school = {State University of New York at Stony Brook},
  author = {Johnson, Michael Thomas},
  year = {2002},
  keywords = {race,Baltimore,Maryland,Social sciences,Antebellum,White supremacy,African-American},
  file = {/Users/elipousson/Dropbox/Zotero/storage/FA79MJVC/Johnson_2002_Black lives and white minds.pdf}
}

