@phdthesis{PhillipsNegroesotherslaves1992,
  address = {United States -- Georgia},
  type = {Ph.{{D}}.},
  title = {"{{Negroes}} and Other Slaves": {{The African}}-{{American}} Community of {{Baltimore}}, 1790-1860},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {"{{Negroes}} and Other Slaves"},
  abstract = {The purpose of this study was to examine the African American community of Baltimore, Maryland, during the seventy years prior to the Civil War, with special comparisons to black populations of Charleston and Philadelphia during the same period. This study traces the social transformation of the city's black population from a transient aggregate of migrant freedpeople, fresh from slavery, to a cohesive community, relatively unwracked by class and intra-racial divisions, inclusive of slaves as well as free Negroes, with strong churches, and boasting the establishment of such exclusively black organizations and associations as fraternal orders, literary associations, and debating societies.
While other urban African American communities founded similar social organizations, each suffered from internal class divisions which caused most of their social institutions to be founded by--and reserved for--the community elite. Baltimore's black community did not depend upon wealth, or its wealthy leadership, for strength; its vitality derived from and depended upon the concord of its entire populace--a populace which was overwhelmingly economically poor. Yet this very economic deprivation allowed Baltimore's African American residents to forge a genuine community with which they not only avoided the divisions of other black urban communities, but allowed them to weather the racial tempests of the antebellum years.
Moreover, because of the unique course of manumissions in Maryland after the American Revolution, Baltimore's black residents were overwhelmingly black, rather than mulatto, and as a result the community was far less stratified by skin color than comparable lower South cities such as Charleston and New Orleans. Through this communal organization and group solidarity, Baltimore's black people had riveted together a cohesive urban community, both free and slave, which looked within for identification, yet which also exerted a centrifugal presence of solidarity that reached and affected black persons throughout the upper South. Perhaps more than any other such population, the story of Baltimore's black people exemplifies the nature of a cohesive community perhaps in its truest sense. Through this community strength, Baltimore's black community was able to resist and survive a rising tide of racial antipathy towards free Negroes which swept the nation, but which was particularly acute in Maryland.},
  language = {English},
  school = {University of Georgia},
  author = {Phillips, Christopher William},
  year = {1992},
  keywords = {Maryland,Social sciences},
  file = {/Users/elipousson/Dropbox/Zotero/storage/ZS7SKRG9/Phillips_1992_Negroes and other slaves.pdf}
}

@phdthesis{Kellypersistentpresencesuccessful2009,
  address = {United States -- Maryland},
  type = {M.{{A}}.},
  title = {The Persistent Presence of a Successful Failure: {{The Maryland State Colonization Society}} and the Importance of Its Existence},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The Persistent Presence of a Successful Failure},
  abstract = {In 1832, the Maryland State Colonization Society officially seceded from the American Colonization Society. With this declaration of independence, the Maryland State Colonization Society embarked on a thirty year journey during which it failed miserably in its very purpose. The Maryland State Colonization Society failed to convince free African Americans to immigrate to Africa in order to pursue a life free of oppression and racism. Nevertheless, the Maryland State Colonization Society sustained an official and financial relationship with the Maryland General Assembly during this period. The thesis explores the underlying motivations and controversies which allowed the Maryland General Assembly to look to the Maryland State Colonization Society as the answer to its own sectional ``crises.'' Through the rhetoric and arguments published in the Maryland Colonization Journal, the Maryland State Colonization Society became a voice for moderation and compromise amongst the divisive and sectional viewpoints of the North and South. Providing a program which slaveholders and anti-slavery proponents could suit to their own beliefs, the white population of Maryland could believe that they had answered the ``problem'' of slavery and a growing free African American population. Through the Maryland State Colonization Society's program, the population of Maryland made a deliberate and prolonged evaluation of slavery's present and future role in the state. Consequently, this allowed the legislatures and leaders of Maryland to make the pragmatic decision to remain in the Union during the Civil War.},
  language = {English},
  school = {University of Maryland, Baltimore County},
  author = {Kelly, Kevin},
  year = {2009},
  keywords = {Maryland,1831-1870,Social sciences,Colonization,Maryland Colonization Journal,Sectionalism},
  file = {/Users/elipousson/Dropbox/Zotero/storage/FH4699UD/Kelly_2009_The persistent presence of a successful failure.pdf}
}

@phdthesis{MorrowOblateSistersProvidence1996,
  address = {United States -- Georgia},
  type = {Ph.{{D}}.},
  title = {The {{Oblate Sisters}} of {{Providence}}: {{Issues}} of Black and Female Agency in Their Antebellum Experience, 1828-1860},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The {{Oblate Sisters}} of {{Providence}}},
  abstract = {The Oblate Sisters of Providence, co-founded by Sulpician priest James Joubert and mulatto Caribbean emigre Elizabeth Clarisse Lange in Baltimore in 1828, formed the first permanent community of African American Roman Catholic women religious in the world. During the 1830s the Oblate Sisters forged the patterns of their communal life, conducted a school for black girls, delineated their complex relationship with the black lay community, and sought acceptance as a legitimate constituency within both the antebellum Roman Catholic Church and southern society.
During the 1840s the Oblate community confronted a series of crises: the death of co-founder James Joubert, the first departures and dismissal of professed members, and apparent desertion by church authorities. For four years the Oblate Sisters persevered in their observance of religious life and the conduct of their school without benefit of official clerical sponsorship. Oblate steadfastness attracted the support of the Redemptorist priests who assumed Oblate spiritual direction in 1847.
During the 1850s the Oblate Sisters expanded their teaching mission, refined their relationship with the black lay community, and reformulated their school curriculum and prospectus to reflect their status as a tested and mature religious community.
The Oblate Sisters both challenged and responded to the attitudes of the Church and the South about race, gender, religion, and ethnicity. Embracing the religious state proved both liberating and empowering to individuals debased by the worldly social order. In renouncing the world to consecrate themselves to God, the Oblate Sisters achieved symbolic release from socially ascribed derogation based on their identities as black women. As Oblate Sisters of Providence, women of color in the antebellum South utilized their piety and spiritual fervor to defy their socially ascribed inferior status and to exercise agency in service to others.
The Oblate Sisters inculcated into their communal consciousness positive senses of themselves as black women and as committed religious. These empowering self-images prevailed for the Oblate membership over both the ambivalence toward them as black women religious plaguing the institutional Church and the disdain toward them as both black and women religious demonstrated by white southern society.},
  language = {English},
  school = {University of Georgia},
  author = {Morrow, Diane Batts},
  year = {1996},
  keywords = {Philosophy,African-Americans,Maryland,Language,literature and linguistics,Social sciences,religion and theology,Catholicism,Joubert,James,Lange,Elizabeth Clarisse,women religious},
  file = {/Users/elipousson/Dropbox/Zotero/storage/2MBM29AP/Morrow_1996_The Oblate Sisters of Providence.pdf}
}

@phdthesis{DiemerBlacknativismAfrican2011,
  address = {United States -- Pennsylvania},
  type = {Ph.{{D}}.},
  title = {Black Nativism: {{African American}} Politics, Nationalism and Citizenship in {{Baltimore}} and {{Philadelphia}}, 1817 to 1863},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Black Nativism},
  abstract = {This dissertation is a study of free African American politics, in the cities of Baltimore and Philadelphia, between 1817 and 1863. At the heart of this black politics were efforts to assert the right of free African Americans to citizenship in their native United States. Claims on the ambiguous notion of citizenship were important to free blacks both as a means of improving their own lives and as a way to combat slavery. The dissertation begins with the organized black protest against the founding of the American Colonization Society. The contest over the notion, advanced by the ACS, that free blacks were not truly American, or that they could not ever be citizens in the land of their birth, powerfully shaped the language and tactics of black politics. The dissertation ends with the enlistment of black troops in the Civil War, a development which powerfully shaped subsequent arguments for full black citizenship. It argues that in this period, free African Americans developed a rhetorical language of black nativism, the assertion that birth on American soil and the contribution of one's ancestors to the American nation, had won for African Americans the right to be citizens of the United States. This assertion was made even more resonant by the increasing levels of white immigration during this period; African Americans pointed to the injustice of granting to white immigrants that which was denied to native born blacks. This discourse of nativism served as a means of weaving the fight for black citizenship into the fabric of American politics. The dissertation also argues that the cities of Philadelphia and Baltimore were part of a distinctive borderland where the issues of slavery and black citizenship were particularly explosive, and where free African Americans, therefore, found themselves with significant political leverage.},
  language = {English},
  school = {Temple University},
  author = {Diemer, Andrew Keith},
  year = {2011},
  keywords = {Philadelphia,Baltimore,Maryland,Pennsylvania,Social sciences,Black nativism,Citizenship,Nationalism},
  file = {/Users/elipousson/Dropbox/Zotero/storage/XAQMWEUT/Diemer_2011_Black nativism.pdf}
}

@phdthesis{TowersRuffiansurbanborder1993,
  address = {United States -- California},
  type = {Ph.{{D}}.},
  title = {Ruffians on the Urban Border: {{Labor}}, Politics, and Race in {{Baltimore}}, 1850-1861},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Ruffians on the Urban Border},
  abstract = {This dissertation analyzes the social history of Baltimore's politics from 1850 to 1861. It focuses on how ward-level conflicts and allegiances shaped Baltimoreans' understanding of the national political debates leading to the Civil War. Local antagonisms stemming from strikes, and racial and ethnic conflict determined allegiance to formal political parties. Local politics were inextricably linked to Baltimore's labor history, race relations, and its street life.
Conservative merchants sympathetic to the South and their allies in the recent immigrant population organized themselves under the banner of the Democratic party. This group fought a coalition of politicians, businessmen, and native-born white skilled workers who shared opposition to the Democrats. The newly created American or Know Nothing party organized these Baltimoreans first around nativism and later Unionism. Best exemplified in election rioting, these groups fought bitterly over control of municipal government and clashed in other contexts, such as work place strikes, and corporate board rooms. Their political division loosely organized more incoherent social differences between different groups of workers, workers and employers, and rival voluntary associations.
Although parties waged this struggle with intensity, partisan conflict masked nativists' and conservative merchants' united opposition to sharing power with African-Americans, unskilled immigrant labor, and women in Baltimore's economy and municipal government. While the most publicized street battles and election riots nominally involved party antagonisms, these battles and other lesser known conflicts warned blacks and immigrants that demands for political or economic power would be violently opposed. Immigrants and blacks fought each other for control of the bottom rungs of the city's economic ladder, while native-born whites turned against immigrants to keep them out of desirable occupations and the political system.
In 1850s Baltimore, comparatively privileged groups fought bitterly for municipal power. Yet Democrats and Know Nothings, excluded the majority of the city from the two main arenas of local power, government and commerce. These conflicts occurred in the context of a spirited and violent public culture. Understanding politics' social basis in cities like Baltimore will better explain the ways that urban changes influenced the national political crises of the Civil War and Reconstruction.},
  language = {English},
  school = {University of California, Irvine},
  author = {Towers, Frank Harold},
  year = {1993},
  keywords = {Maryland,Social sciences},
  file = {/Users/elipousson/Dropbox/Zotero/storage/79RS28NN/Towers_1993_Ruffians on the urban border.pdf}
}

@phdthesis{WolffRacialimaginingsSchooling1998,
  address = {United States -- Minnesota},
  type = {Ph.{{D}}.},
  title = {Racial Imaginings: {{Schooling}} and Society in Industrial {{Baltimore}}, 1860-1920},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Racial Imaginings},
  abstract = {Race is a central motif in American history but is seldom acknowledged as such because whiteness remains normative in popular conceptions of national identity, This dissertation argues that as republican ideals faded in the nineteenth century, the native-born (`white') middle-class fashioned a national identity fused with racial beliefs to distinguish themselves not just from African-Americans but from southern and eastern European immigrants that they considered racially different as well. Nations, wrote Benedict Anderson, are `imagined communities'; in the United States that national identity has always been seen through the prism of race. Slave emancipation, southern and eastern European immigration, and industrialization cast middle-class social reform and welfare movements\textemdash{}what Robert Wiebe called `the search for order'\textemdash{}in racialized terms. Based upon a community study of Baltimore, this study demonstrates that schooling in the American past has been vastly underrated as a social and cultural process in which identities were formed and contested. Not every American grows up to join a labor union, raise a family, or vote, but nearly all of them go to school as children. Before the 1890s, schools before the 1890s, schools flew no American flag, children recited no `Pledge of Allegience,' and pupils learned little about American history; yet in that decade schools were suddenly infused with the spirit of racial nationalism. While lessons in History, Geography, and Civics reinforced the supposed superiority of `the American race,' vocational training prepared working-class children for factory employment. While clashes over race and ethnicity gave the schooling system its shape, the cultural imprint of whiteness and American nationalism were broader phenomena affecting the terms of public discourse throughout the twentieth century.*
A CD-ROM is included with dissertation. *Originally published in DAI Vol. 58, No. 12. Reprinted here with added notation.},
  language = {English},
  school = {University of Minnesota},
  author = {Wolff, Robert S.},
  year = {1998},
  keywords = {Education,Baltimore,Maryland,1870-1905,Social sciences,Identity,Industrial,Racial imaginings,Schooling,Society},
  file = {/Users/elipousson/Dropbox/Zotero/storage/H9UMUFDJ/Wolff_1998_Racial imaginings.pdf}
}

@phdthesis{MoyerGraciouscarelessRace2010,
  address = {United States -- Maryland},
  type = {Ph.{{D}}.},
  title = {Gracious but Careless: {{Race}} and Status in the History of {{Mount Clare}}},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Gracious but Careless},
  abstract = {Historic plantation sites continue to struggle with the legacy of slavery and black history, particularly concerning their significance in American culture. Although enslaved persons are erased from the contemporary landscape of Carroll Park in Baltimore, Maryland, the historical and archaeological record preserves their importance to the Carroll family and the plantation called Georgia or Mount Clare. I argue that historic preservation is a form of social justice when underrepresented historical groups are integrated into interpretations of historical house museums and landscapes. Enslaved blacks held essential roles in every aspect of Mount Clare from circa 1730 to 1817. They became culturally American at the intersection of race and status, not only through the practice of their own cultural beliefs and values, but those of elite whites, as well. Focus on white ancestors reveals only part of the history of Mount Clare: I demonstrate that blacks' own achievements cannot be ignored.},
  language = {English},
  school = {University of Maryland, College Park},
  author = {Moyer, Teresa S.},
  year = {2010},
  keywords = {Slavery,race,Baltimore,Chesapeake,Maryland,Social sciences,Communication and the arts,Carroll Park,Historic house,Mount Clare},
  file = {/Users/elipousson/Dropbox/Zotero/storage/38U8MNFV/Moyer_2010_Gracious but careless.pdf}
}

@phdthesis{HagamanPersonalbattleslives2004,
  address = {United States -- Illinois},
  type = {Ph.{{D}}.},
  title = {Personal Battles: {{The}} Lives of {{Maryland}}'s Black {{Civil War}} Veterans, 1840\textendash{}1920},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Personal Battles},
  abstract = {There have been multiple studies of the lives of black Civil War soldiers. Most argue that the wartime experience of black enlistees elevated and ennobled them. Subsequent to release from service, these studies continue, black veterans tended to occupy positions of relative social, political and economic importance within the black community. This current study tests the prevalent belief that service during the Civil War was, on the whole, a positive experience.
This study uses a sample of 290 black Civil War veterans from Maryland. Almost all bibliographical information about these veterans was culled from pension records.
The results of this dissertation do not support the contention that service during the Civil War improved the lives of black veterans after the war. Black veterans often left the service in bad physical and psychological condition and this, in turn, precluded their ability to work in jobs that required heavy manual labor. Many black veterans, therefore, were often dependent upon the charity and good will of others as well as the pension bureau. Ironically, however, the debilities from which many veterans suffered compelled them to seek help and this, in turn, helped bolster the size and complexity of Maryland's black community.},
  language = {English},
  school = {Northern Illinois University},
  author = {Hagaman, Robert A.},
  year = {2004},
  keywords = {Maryland,Social sciences,Black veterans,Civil War veterans},
  file = {/Users/elipousson/Dropbox/Zotero/storage/VJRE2W4M/Hagaman_2004_Personal battles.pdf}
}

@phdthesis{KuhnMarylandmoderateconundrum2015,
  address = {United States -- Maryland},
  type = {M.{{A}}.},
  title = {Maryland and the Moderate Conundrum: {{Free}} Black Policy in an Antebellum Border State},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Maryland and the Moderate Conundrum},
  abstract = {The following examines the complexities of slavery in Maryland in the antebellum period and argues that as a result of Maryland's geographic location as a border between North and South, Maryland slaveholders desperately clung to the institution and attempted to shape their world into a slave society, regardless of the fact that slavery had long been dying out as an economic necessity. In the process, they called for federal protection of fugitive slave property, subscribed to a strict code of white southern conduct, and attempted to weed out any threat to slavery\textemdash{} mainly the state's large free black population. This concept is intended to argue against the idea that Maryland was a middling ground where ties to slavery were somehow weak or insignificant as a result of economic forces. As the Civil War approached, Maryland slaveholders in fact hardened the institution at the expense of Maryland's African American population.},
  language = {English},
  school = {University of Maryland, Baltimore County},
  author = {Kuhn, Talbot Anne},
  year = {2015},
  keywords = {Slavery,Maryland,1831-1870,Social sciences,Antebellum,Slaveholders},
  file = {/Users/elipousson/Dropbox/Zotero/storage/XS58HDGJ/Kuhn_2015_Maryland and the moderate conundrum.pdf}
}

@phdthesis{FletcherCityDeadcolored2013,
  address = {United States -- Maryland},
  type = {Ph.{{D}}.},
  title = {The {{City}} of the {{Dead}} for Colored People: {{Baltimore}}'s {{Mount Auburn Cemetery}}, 1807\textendash{}2012},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The {{City}} of the {{Dead}} for Colored People},
  abstract = {Dead for Colored People: The Creation of Mount Auburn Cemetery explores the common theme of African American history-the struggle for freedom and autonomy-via the African American cemetery. This study first focuses on how African Americans in Baltimore, MD agitated and succeeded in establishing African American burial rights. Secondly, it argues that these burial rights led to African Americans obtaining freedom and autonomy. This study is specifically situated on Mount Auburn Cemetery, located in South Baltimore, and examines the numerous social and historical factors that shaped, transformed, and ultimately led to a small African American burial ground becoming a thirty-four acre cemetery, a social institution, and a business. Starting in 1807, seven African Americans bought two and one-fourth acres of land giving African Americans, free and enslaved, a right to freedom through death. African Americans could not control their enslaved and marginalized lives, but they could control their deaths. Post emancipation, the cemetery strategized a moved to South Baltimore, bought more land, and created a symbiotic relationship with a newly formed African American community by the name of Hullsville. The cemetery professionalized and became a business paving the way for independent African American morticians.
It is important to note that this dissertation is not a narrow history of some obscure cemetery that fell into disarray. Instead, it places Mount Auburn Cemetery as a unit of analysis in order to do the following: a) illustrate the historical significance of Mount Auburn Cemetery to the African American community; b) study nineteenth century and twentieth century race relations between Blacks and Whites, especially the relationship involved within the origins of the cemetery; c) understand the significance of African American cultural norms and the interconnectedness of death and funerary practices within the Black community.},
