}

@phdthesis{erdman_eyes_2007,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {“{{Eyes}} of the world”: {{Racial}} discrimination against {{African}} dignitaries along {{Maryland Route}} 40 during the {{Kennedy Administration}}},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {“{{Eyes}} of the world”},
  abstract = {During the Kennedy Administration, several African dignitaries were victims of discrimination along the Maryland portion of U.S. Route 40. Causing tension both in the Civil Rights Movement and the Cold War, this discrimination prompted the federal government to create the Special Protocol Service Section, which dealt with the incidents and tried to come up with solutions to the problem. The incidents prompted the SPSS, led by Pedro Sanjuan, to heavily support a public accommodations law that would outlaw discrimination in any of Maryland's public accommodations, which included motels, hotels, and restaurants. Such federal intervention into a state and local matter caused conflict between Marylanders and members of the State Department. Press and propaganda also played an important role in the Route 40 ordeal. Soviet propaganda used the discrimination to try and sway new African countries to the Russian side of the Cold War.
By examining primary sources, including State Department documents and Baltimore area newspapers, this thesis looked at the aspects of the Route 40 discrimination. Drawing most of the primary sources from the John F. Kennedy Presidential Library, the thesis provides a unique perspective on how the government dealt with the entire issue of the Route 40 discrimination.
This thesis shows how the incidents on Route 40 thrust Maryland into the national and international scene by examining the incidents themselves, the government's response and how these played in both the domestic and international press.},
  language = {English},
  timestamp = {2016-08-26T20:28:44Z},
  urldate = {2016-08-26},
  school = {Morgan State University},
  author = {Erdman, Jennifer Lynn},
  year = {2007},
  keywords = {Social sciences},
  file = {Erdman_2007_“Eyes of the world”.pdf:/Users/elipousson/Dropbox/Zotero/storage/CUUK9XXU/Erdman_2007_“Eyes of the world”.pdf:application/pdf}
}

