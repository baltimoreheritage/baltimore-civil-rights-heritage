@phdthesis{johnson_strayer_2012,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {The {{Strayer}} survey and the colored schools of {{Baltimore City}}, 1923–1943},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {Since a quality education was believed to be a gateway to a productive future, members of Baltimore City's African American community sparred continuously with school officials for equal access to educational facilities and opportunities throughout the late nineteenth and early twentieth centuries. From 1920 to 1921, George D. Strayer surveyed the Baltimore City Public School. His report documented, in detail, the deplorable school conditions for African American educators and students. Importantly, the recommendations of the school survey led to monumental changes within the Baltimore City Division of Colored Schools between 1923 and 1943. This thesis analyzes school reform efforts from an educational reformist‘s perspective and its impact on providing a quality, albeit still segregated education for African Americans living in Baltimore City until World War II. This thesis, which relies on the Strayer Report, The Baltimore Afro-American and The Baltimore Sun newspapers, and official school reports for primary sources, demonstrates how school reform efforts provided African American educators with the necessary tools to improve the quality of education for African American students in the Baltimore City School System during legal segregation.},
  language = {English},
