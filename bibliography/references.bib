@phdthesis{williams_living_1998-1,
  address = {United States -- Pennsylvania},
  type = {Ph.D.},
  title = {Living just enough in the city: {{Change}} and activism in {{Baltimore}}'s public housing, 1940-1980},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Living just enough in the city},
  abstract = {Using the historical case study of Baltimore, Living Just Enough in the City focuses on the dynamism of public housing communities--the people, their activism, and the places (both low-rise and high-rise)--between 1940 and 1980. The dissertation analyzes the social experiences and grassroots activism of tenants, particularly African American women and men, as a way to unveil the changing landscapes of public housing and to explore how urban policy shaped black working people's lives. Public housing was an integral part of the black working-class experience and became a critical policy tool for rehousing--and (re)segregating--black people already concentrated in and increasingly migrating to inner cities in post-1940 America.
Living Just Enough in the City is simultaneously a documentation of and an exploration into the spirit and nature of black working class activism. Tenants' concerns and responses reflected their desire to address daily and/or practical needs as well as transcend their marginal status within a raced, gendered and classed hierarchy. An examination of tenants' lives and their activism on the local level shows that people struggled wherever they were, and that their struggles, which varied in content and form over time, were often a part, through not necessarily simply a reflection of, larger social movements. The focus on tenant activism, therefore, also provides a lens into broader historical events such as workers' unrest during the 1940s, civil rights struggles beginning in the 1940s and 1960s, and black power movements in the 1960s. Living Just Enough in the City, then, offers a unique perspective into the changes in and nature and limits of public housing; documents the agency of a group of people who have often been ignored or viewed as passive; and contributes to the literature on urban history, working-class activism, women's studies, social movements, social policy, and race, gender and class.},
  language = {English},
  timestamp = {2016-08-27T01:26:09Z},
  urldate = {2016-08-27},
  school = {University of Pennsylvania},
  author = {Williams, Rhonda Yvette},
  year = {1998},
  keywords = {African-Americans,Maryland,Social sciences,urban history,working class},
