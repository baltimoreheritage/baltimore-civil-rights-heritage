@phdthesis{doster_strike_2015,
  address = {United States -- Maryland},
  type = {Ph.D.},
  title = {"{{To Strike}} for {{Right}}, {{To Strike With Might}}": {{African Americans}} and the {{Struggle}} for {{Civil Rights}} in {{Baltimore}}, 1910-1930},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {"{{To Strike}} for {{Right}}, {{To Strike With Might}}"},
  abstract = {“‘To Strike for Right, To Strike With Might’: African Americans and the Struggle for Civil Rights in Baltimore, 1910–1930” examines the nature, character and scope of early civil rights activism among African Americans in Baltimore, Maryland. Utilizing an expansive definition of “civil rights”, it explores not only voting and holding political office, access to public education, and fair housing opportunities; it also considers struggles for access to municipal and social services and struggles related to labor and employment. By placing all of these terrains of struggle under the umbrella of “civil rights”, the dissertation emphasizes the importance of these rights in relation to one another and their importance in the minds and lives of African Americans who struggled for rights in each of these categories as part of a broader struggle for equality.
Baltimore has long been recognized for its civil rights activism by scholars who portray the era of the 1930s to 1950s as a kind of “golden age” of civil rights activism in the city, considering such activism to have been dormant prior to that period. The dissertation reveals an active civil rights movement in the city in the decades preceding the 1930s that was led primarily by members of the middle-class but drew widespread support and strength from members of all classes in Baltimore’s African American community. In uncovering the civil rights activism of the period from 1910 to 1930, the dissertation brings to the forefront previously ignored organizations, including the Federation of Parent-Teacher Clubs, the Women’s Cooperative Civic League, the Independent Republican League, and the Baltimore Urban League. It also reveals that the activism of the period from 1910 to 1930 was important in launching major civil rights campaigns of national organizations such as the NAACP, whose residential segregation campaign had roots in the fight in Baltimore. Throughout, the dissertation explores the ways that black Baltimoreans defined priorities and struggled for rights, resulting in a more nuanced understanding of African Americans’ struggles for citizenship and equality at beginning of the twentieth century.},
  language = {English},
  timestamp = {2016-08-26T19:58:16Z},
  urldate = {2016-08-26},
  school = {University of Maryland, College Park},
  author = {Doster, Dennis Anthony},
  year = {2015},
  keywords = {African american,Baltimore,Civil rights,Social sciences}
}

@phdthesis{kennedy_victorine_2012,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {Victorine {{Adams  The Civil Rights}} and {{Social Justice Movement}} in {{Baltimore}}, {{Maryland}}},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {Victorine Adams is a Civil Rights and Social Justice activist from Baltimore, Maryland, whose actions and accomplishments have largely been ignored in the analysis of the Civil Rights Movement. She is reflective of a multitude of women who buttressed the Movement through behind-the-scene activism, such as fundraising, and voter registrations. However, like many other women who worked with the Civil Rights Movement her activism was not limited to civil rights. Rather Victorine Adams worked with a number of secular and religious groups to create equality, to provide resources for women and girls, and to assist people in poverty.
The addition of her life to the historical record expands the understanding of the Civil Rights Movement as a dynamic process connected to other movements for change and justice; that it was a movement that spanned much longer than the 1950s and 1960s. It reflects the complicated nature of the fight for equality as Adams' husband, William L. Adams, former illegal gambling runner, and later one of the wealthiest men in Baltimore through his venture capitalist business, financially supports Victorine's activism along with others in Baltimore. It also adds support to the idea that religion needs to be included in an understanding of many of the men and women who worked with the Civil Rights Movement.},
  language = {English},
  timestamp = {2016-08-26T19:59:29Z},
  urldate = {2016-08-26},
  school = {University of Maryland, Baltimore County},
  author = {Kennedy, Kellian Elizabeth},
  year = {2012},
  keywords = {Baltimore,Civil rights,Maryland,Social justice,Social sciences,Urban Commission of Baltimore,Victorine Adams},
  file = {Kennedy_2012_Victorine Adams The Civil Rights and Social Justice Movement in Baltimore,.pdf:/Users/elipousson/Dropbox/Zotero/storage/X9DSPRX2/Kennedy_2012_Victorine Adams The Civil Rights and Social Justice Movement in Baltimore,.pdf:application/pdf}
}

@phdthesis{thompson_civil_1996,
  address = {United States -- Maryland},
  type = {Ph.D.},
  title = {The civil rights vanguard: {{The NAACP}} and the {{Black}} community in {{Baltimore}}, 1931-1942},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The civil rights vanguard},
  abstract = {The search for the origins of the civil rights movement has moved to the grassroots level. Initial studies indicate that local blacks and their communities in the Deep South and Upper South began mobilizing for civil rights as early as the early 1940s. To date, nobody has studied black community mobilization in the Border South or the role of the National Association for the Advancement of Colored People (NAACP) in this historical process. This dissertation, a case study of the origins and early development of the civil rights movement in Baltimore, attempts to fill these historiographical voids.
The story of Baltimore's civil rights movement begins with the coalescence of Baltimore's black community in response to segregation. Young blacks coming of age in the midst of the Great Depression organized the City-Wide Young People's Forum, and in their search for equal employment opportunities they renewed the struggle for black civil rights in Baltimore. A buy-where-you-can-work boycott mobilized black community support and showcased future local and national civil rights leaders. Afterwards, local activists teamed with the national office of the NAACP to fight segregation at the University of Maryland and to revive the Baltimore branch of the NAACP. Black Baltimoreans continued their civil rights struggle through the Baltimore NAACP, and they spread the movement to other parts of Maryland, while working with the national NAACP lawyers to experiment with desegregating high schools and equalizing teachers' salaries. By 1942, Baltimore's civil rights leaders were able to mobilize and lead 150 community organizations and 2,000 citizens on a March on Annapolis to protest police brutality. Thus, we find that Baltimore, a Border South city, was in the vanguard of the civil rights movement at both the grassroots and national levels before America's participation in World War II.},
  language = {English},
  timestamp = {2016-08-26T20:00:23Z},
  urldate = {2016-08-26},
  school = {University of Maryland, College Park},
  author = {Thompson, Bruce A.},
  year = {1996},
  keywords = {African-Americans,Maryland,Social sciences},
  file = {Thompson_1996_The civil rights vanguard.pdf:/Users/elipousson/Dropbox/Zotero/storage/APSFN4Z4/Thompson_1996_The civil rights vanguard.pdf:application/pdf}
}

@phdthesis{maxwell_black_2014,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {Black faces in blue uniforms: {{Desegregating}} the {{Baltimore}} police department 1920–1950},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Black faces in blue uniforms},
  abstract = {In 1937, the Baltimore city police department hired its first African-American officer. Her hire and the subsequent hire, seven months later, of four black male officers represented the culmination of nearly two decades of civil rights activism by a coalition of print media, middle-class reformers, and cooperative politicians.
I argue that civil rights activity in Baltimore, especially in the first half of the twentieth century, was generally nonviolent and led by a strong-willed, generally secular cadre of middle-class African Americans. In addition, because of its border state status, racial gains occurred earlier in Baltimore than they did in Dixie, but not as early as they did in more northern cities. The leaders of Baltimore's civil rights movement used their political and journalistic influence to call for and effect change. The process of desegregating the city's police department is a clear example of this.
I begin with a general narrative of the campaign for desegregation between 1920 and 1950. Using the reporting and editorializing of the Baltimore Afro-American, I explain the importance of desegregation to the community, not only as a tangible example of racial progress, but as acknowledgment of the rising civic power of African-American Baltimoreans.
I explore issues related to the police department and its integration, including the racial and sexual power dynamics inherent between any police force and its community, especially in the United States in the nineteenth and twentieth centuries. I also examine the economic class of the activists and the first few officers as a means of reiterating the fundamentally middle-class nature of this particular civil rights campaign.},
  language = {English},
  timestamp = {2016-08-26T20:06:47Z},
  urldate = {2016-08-26},
  school = {University of Maryland, Baltimore County},
  author = {Maxwell, Megan Christine},
  year = {2014},
  keywords = {African-American,Baltimore,Maryland,Police department,Racial desegregation,Social sciences},
  file = {Maxwell_2014_Black faces in blue uniforms.pdf:/Users/elipousson/Dropbox/Zotero/storage/B2NIN25Z/Maxwell_2014_Black faces in blue uniforms.pdf:application/pdf}
}

@phdthesis{ahmed-williams_st._2007,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {St. {{Peter Claver Catholic Church}}:  {{Civil}} rights activism and surviving the decline of a {{West Baltimore}} community},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {St. {{Peter Claver Catholic Church}}},
  abstract = {This study examines the role of the Catholic Church in Baltimore's African-American community focusing on one institution in particular--St. Peter Claver Catholic Church and its West Baltimore community. Dedicated in 1888, the church has played an important role for over 118 years, contributing to the religious, educational, and social aspects of its congregation and the surrounding community. Specifically, this study examines the church's role in the community during the civil rights era as well as its post-desegregation efforts to develop alternatives in education, housing, and social conditions, which would help it survive and continue to serve those left in its declining parish and community.
This study provides an overview of the role of St. Peter Claver in the African-American community, including its history and development as a parish. This study explores how the church served the members of its parish and contributed to the social change that occurred during the turbulent time of the civil rights movement, including the involvement of parish members and priests in local and national protest and activities. It discuses how the legal and social victories of the civil rights movement contributed to the desegregation of residential communities as well as public and private accommodations, which in turn contributed to the decline in the stability of the community that the church served.
Further, the study examines the strategies that St. Peter Claver developed including participating in cluster schools, developing partnerships with non-Catholic community organizations and organizing new ministries to fit the changing needs of the impoverished community it served, while continuing its dedication and mission to convert souls for Christ and to serve and educate African-Americans. The activities of its members and priests are viewed as a collective endeavor and the impact of the civil rights era and desegregation is evaluated by its effect on the church and its ministries as a whole.
This study uses a conventional narrative to present findings of primary sources from the archives of The Society of St. Joseph of the Sacred Heart (Josephite archives) that started the St. Peter Claver Parish and oral interviews. This study exposes aspects of an African-American Catholic church experience during the civil rights era, desegregation period, and decline of its community. The thesis highlights the trials, spirit, determination, and triumphs of St. Peter Claver Catholic Church of Baltimore.},
  language = {English},
  timestamp = {2016-08-26T20:07:46Z},
  urldate = {2016-08-26},
  school = {Morgan State University},
  author = {Ahmed-Williams, Efa},
  year = {2007},
  keywords = {Maryland,Social sciences},
  file = {Ahmed-Williams_2007_St.pdf:/Users/elipousson/Dropbox/Zotero/storage/KRADFES5/Ahmed-Williams_2007_St.pdf:application/pdf}
}

@phdthesis{tilghman_no_2012,
  address = {United States -- District of Columbia},
  type = {Ph.D.},
  title = {No {{Road}} to {{Renaissance}}: {{Black Protest}} and {{Downtown Expansion}} in {{Baltimore}}, 1954–1977},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {No {{Road}} to {{Renaissance}}},
  abstract = {"No Road to Renaissance: Black Protest and Downtown Expansion in Baltimore 1954-1977" examines African American postwar protest movements and community building during the redevelopment of downtown Baltimore. The period analyzed, 1954-1977, is considered the traditional era of the Civil Rights /Black Power movement. This research highlights the evolution of a power struggle between the city's power elite and black residents over the racial and class makeup of the city.
This dissertation's thesis is that year of 1954, the time of the Brown v. Board decision issued by the U.S. Supreme Court, was the beginning of challenges for control between city politicians and business leaders political and the African American community over housing, land, electoral politics, public and social spaces, and legislation in relation to downtown expansion. However, the power struggle was not a long or continuous. There were distinctions of such protest campaigns before and after the 1954 Brown v. Board decision. In Baltimore, from 1954 through 1965, the black middle class, local civil rights organizations, and student activists engaged in blockbusting, voter registration, black electoral politics, and student sit-ins in downtown department stores. Although these measures never challenged downtown urban planning directly, they changed the racial and class makeup of a city. Beginning in 1966, black grassroots organizations and working class white communities directly challenged legislation and urban planners that related to downtown redevelopment. Such campaigns included the protest for the stability of neighborhoods threatened by massive highways and exclusion from the decision making process of the creation downtown projects such as the Charles Center/ Inner Harbor.
The vision of a post-industrial economy in Baltimore was created by downtown businessmen and city politicians that consisted of corporate services and tourism via Charles Center/Inner Harbor as an attempt to draw upper class white residents from the Maryland suburbs back into the city. From 1954 through 1960, the effect of blockbusting practices and the growing voting strength within black city districts in West Baltimore led to the first city-wide hysteria over the changing racial profile of the city. From 1960 through 1964, at the time of the creation of the Charles Center, Morgan State College student activists desegregated all downtown department stores and restaurants while the black middle class residents challenged housing developers and politicians over segregated housing near the downtown area. From 1966 through 1977, downtown urban planning was challenged directly by black grassroots activists over displacement of blacks from areas near the central business district. Meanwhile, black and white anti-highway organizations protested the East-West Expressway.
This study shows how important the national Civil Rights Movement, beginning with the 1954 Brown v. Board decision, had an influence on an urban center like Baltimore. Moreover, the Modern Civil Rights/Black Power Movement is "periodized" in the time frame of 1954 through 1977. This study also shows the role that U.S. government played in urban affairs, how the transformation of a local economy impacted the lives and communities of black residents, and how blacks responded exclusion and exploitation. Downtown expansion and postwar social movements in Baltimore is presented as a prototype of how to place downtown reconstruction into the historiography of African American Urban History since World War II.},
  language = {English},
  timestamp = {2016-08-26T20:08:03Z},
  urldate = {2016-08-26},
  school = {Howard University},
  author = {Tilghman, John R.},
  year = {2012},
  keywords = {Anti-freeway construction,Baltimore,Black power,Black removal,Civil rights,Civil rights movement,Downtown redevelopment,Maryland,Social sciences}
}

@phdthesis{cumberbatch_working_2001,
  address = {United States -- Connecticut},
  type = {Ph.D.},
  title = {Working for the race:  {{The}} transformation of the civil rights struggle in {{Baltimore}}, 1929–1945},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Working for the race},
  abstract = {Working For the Race: The Transformation of the Civil Rights Struggle in Baltimore, 1929–1945, traces the evolution of African American civil rights organizations, and the efforts of middle-class leaders to incorporate working-class concerns into their political programs. The period 1929 to 1945 stands as one of the key social and political eras in the history of Baltimore. During these sixteen years, civil rights activists debated whether or not their chances for success would be better if they formed alliances with radical political organizations, concentrated on intraracial campaigns, or placed their hopes in the policies of the federal government. Ultimately, African American activists in Baltimore tried all three of these strategies, incorporating aspects of their political campaigns, and reformulating them in a way that was more responsive to the immediate concerns of their community, be they mob violence or the treatment of workers in the steel mill.
Chapter one focuses the relationship between the Baltimore branch of the National Association for the Advancement of Colored People, the Afro American newspaper, and the Communist Party. Chapter two moves the discussion from specific organizations to the incidents of mob violence on Maryland's Eastern Shore. Chapter three examines the African American community's response to Communist Party programs, and the emergence of a youth movement that would ultimately lead to the resurgence of political activism among black Baltimoreans. Chapter four marks the period when middle-class leaders in the African American community directed their attention away from the Communists and focused on working with New Deal agencies during the Great Depression.
Chapter five traces the re-emergence of the Baltimore NAACP, its conflicts with the Communist Party, and its campaign to desegregate higher education. Chapter six examines the relationship between the African American community and Congress of Industrial Organizations at Bethlehem Steel's Sparrows Point plant and its three major shipyards in the area. Working with both the Fair Employment Practices Committee and the CIO, civil rights leaders were able to break through racial barriers that had historically confined African Americans to the least paying and least desirable jobs.},
  language = {English},
  timestamp = {2016-08-26T20:08:17Z},
  urldate = {2016-08-26},
  school = {Yale University},
  author = {Cumberbatch, Prudence Denise},
  year = {2001},
  keywords = {Baltimore,Civil rights movement,Maryland,race,Social sciences}
}

@phdthesis{jackson_discrimination_2008,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {Discrimination deferred: {{How}} the {{Kerr V}}. {{Pratt}} (1945) case contributed to the end of segregation},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Discrimination deferred},
  abstract = {The Kerr v. Pratt (1945) decision was monumental in helping to dismantle institutionalized segregation in Baltimore that was practiced by many of the city's public institutions. This case illustrates how African-Americans in Baltimore began to achieve their full-fledged Civil Rights by using the concept of unfair employment practices as a stepping stone for achieving complete racial integration nationally.
This paper will examine this case from a Black Nationalist perspective and will emphasize the 1945 District Court of Maryland case, analyzing its impact in assisting African-Americans in Baltimore to achieve progress educationally or occupationally. This study is based on primary sources including reports in the Baltimore Sun and the Afro-American newspapers, law reviews, articles and other primary sources such as legal dockets, letters, report cards and receipts. In addition, Louise Kerr the defendant of the case and Law Professor Larry Gibson were interviewed to help provide essential information to this work.
This study is important because it illustrates how Kerr and her community helped to lay the groundwork for later Civil Rights achievements in American courts. The Kerr v. Pratt (1945) raised major questions about race, segregation and institutional discrimination in the Baltimore public sector. Hence, its ramifications were far-reaching and received national acclaim as it contributed to the renowned Brown v. Board of Education (1954). This case shows how ordinary citizens like Louise Kerr and prominent jurist like Charles Hamilton Houston worked together to bring social and economic change to the oppressed community of Baltimore.},
  language = {English},
