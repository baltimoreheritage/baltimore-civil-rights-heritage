  author = {Wolff, Robert S.},
  year = {1998},
  keywords = {1870-1905,Baltimore,Education,Identity,Industrial,Maryland,Racial imaginings,Schooling,Social sciences,Society},
  file = {Wolff_1998_Racial imaginings.pdf:/Users/elipousson/Dropbox/Zotero/storage/H9UMUFDJ/Wolff_1998_Racial imaginings.pdf:application/pdf}
