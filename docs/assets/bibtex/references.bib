@phdthesis{henderson_local_1994,
  address = {United States -- Maryland},
  type = {Ph.D.},
  title = {Local Deals and the {{New Deal}} State: {{Implementing}} Federal Public Housing in {{Baltimore}}, 1933-1968},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Local Deals and the {{New Deal}} State},
  abstract = {Historians who have studied the American state have focussed primarily on Washington, D.C., particularly for the period beginning with the New Deal. First, they examined the role of Franklin Roosevelt in New Deal politics. Subsequently, they argued that New Deal policymaking combined the associative state developed in the 1920s with a more elaborate, though incomplete, welfare state. Recent historians have concentrated on the role of federal officials who developed and elaborated policy--"political entrepreneurs" who sold public policies as ways to solve specific social problems while building their own organizations. What has been largely ignored in this analysis is the manner in which officials actually implemented programs, and localities shaped policy in the process. This study of public housing examines how Federal policies were reforged in "local deals," a process that was one of the major characteristics of the New Deal and its aftermath.
In arenas that took a variety of forms, policy implementation in America involved a complex process of community politics and Federal-community negotiations, with local structures shaping the process and the outcome. In the case of public housing policy, municipal political organization, local political culture, the vernacular housing stock, and the urban economy shaped local policy contests. Politicians, businesses, civic groups, neighborhoods and households connected with the public housing program in different ways. Each developed a distinctive strategy to promote, contest, use, or subvert it. It was in the resulting negotiations and interactions that public housing flourished, died, or was cast.
Meanwhile, the program in turn reshaped local political arrangements. New groups mobilized to support the program or pose policy alternatives. Old players developed new tactics. While some grew alarmed at the increasing federal presence, antistatism inexorably gave way to a process in which the federal programs became permanent, and the politics became intergovernmental. Political actors adapted. They thus defined the new program, and then engaged it in a manner that made policymaking continual and inclusive of nearly every aspect of the public housing program as a variety of critical problems emerged.},
  language = {English},
  timestamp = {2016-10-06T01:37:45Z},
  urldate = {2016-08-27},
  school = {The Johns Hopkins University},
  author = {Henderson, Peter Harry},
  year = {1994},
  keywords = {federal policies,local politics,Maryland,Social sciences},
  file = {Henderson_1994_Local deals and the New Deal state.pdf:/Users/elipousson/Dropbox/Zotero/storage/H6U637VD/Henderson_1994_Local deals and the New Deal state.pdf:application/pdf}
}

@phdthesis{hale_devoted_2002,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {Devoted Dissidents:  {{The}} Black Feminist Founding of {{Douglas Memorial Community Church}}, 1925–1948},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Devoted Dissidents},
  abstract = {Through the extensive use of church records, this thesis explores the leadership of Black women in the establishment of a Baltimore Christian church. A Black feminist paradigm is used to interpret the history of this institution because the paradigm encompasses the ethnic history of African Americans as well as the issue of gender equality within the Black community. The paradigm asserts that males as well as females can be feminists, and that Black men are historically sensitive to the struggles of women for gender equality. Thus the early history of Douglas Memorial Community Church, as a congregation founded by a woman and led by a pastor who recognized the ministerial gifts of women, is well suited for this paradigm.
Founded in 1925 by Mary E. Jones, the church was originally named Cosmopolitan African Methodist Episcopal (AME) Church. Jones, who was president of the Trustee Aid Society of the Bethel AME Church, scheduled a meeting with Bethel dissidents, and requested that Rev. Frederick Douglas serve as pastor. In 1938, the church was renamed Douglas Memorial Community Church.
My thesis will explore the contributions of women to the founding and organization of the congregation during the Douglas pastorate, 1925–48. I argue that women were tremendously empowered under Douglas' pastorate. He penned the Black Community Church movement creed that woman could serve as pastors. This doctrinal statement was quite radical since traditionally the office of pastor is reserved for men.},
  language = {English},
  timestamp = {2016-08-27T01:29:43Z},
  urldate = {2016-08-27},
  school = {Morgan State University},
  author = {Hale, Miss},
