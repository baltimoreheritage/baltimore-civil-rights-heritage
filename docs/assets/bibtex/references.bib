  keywords = {1905-1930,1930-1954,Maryland,Social sciences},
  file = {Hale_2002_Devoted dissidents.pdf:/Users/elipousson/Dropbox/Zotero/storage/52E5C5AU/Hale_2002_Devoted dissidents.pdf:application/pdf}
}

