@phdthesis{may_residential_1999,
  address = {United States -- Maryland},
  type = {Ph.D.},
  title = {The Residential Change of the {{Free Black}} Population of {{Baltimore}}, 1850-1860},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {The antebellum Free Black population represented a marginal segment of Southern society—a gray area in a Black and White caste structure. Nowhere was this unique status more evident than in the cities of the upper South such as Baltimore, as manumitted slaves were increasingly drawn to the opportunities provided by the urban environment. During the early decades of the nineteenth century, Baltimore's Free Black population increased dramatically, outpacing nearly every other group. In the final antebellum decade, however, this growth was virtually halted. The combination of economic recession, labor competition from the increasing immigrant population, and growing social tensions created an environment of significant change for Free Blacks. While much scholarly attention has been devoted to various aspects of this community's growth, little has focused on its changing distribution throughout the city during this crisis. The specific purpose of this study is to provide insight into the role of occupation as a catalyst for the residential change in the Free Black population of Baltimore during the 1850s. The changing distributions of the Free Black population are shown at three scales in 1850 and 1860: ward level aggregates, a citywide sample of households, and neighborhood case studies. This methodology matches the spatial data of city directories to the demographic data of the manuscript census and provides a reliable basis for pinpointing a sample of households. The block-level analysis displays the various processes of micro-scale residential turnover which resulted from occupational competition between Free Blacks and immigrants. Factors such as occupational diversity and social support networks are identified as particularly important determinants in the success of small Free Black communities to withstand such residential competition. On a citywide scale, the samples exhibit clusters of racial segregation which resulted from these processes, both within and between city wards. Overall, the resulting patterns suggest a significant shift of the Free Black population from the southeastern waterfront regions, where water and construction trades were in decline, to new emerging cores in the northeastern interior dominated by service and transportation occupations.},
  language = {English},
  timestamp = {2016-09-05T00:47:29Z},
  urldate = {2016-09-05},
  school = {University of Maryland, College Park},
  author = {May, Patrick Joseph},
  year = {1999},
  keywords = {Baltimore,Black population,Free Black population,Maryland,Residential change,Social sciences},
  file = {May_1999_The residential change of the Free Black population of Baltimore, 1850-1860.pdf:/Users/elipousson/Dropbox/Zotero/storage/NE8I5R6P/May_1999_The residential change of the Free Black population of Baltimore, 1850-1860.pdf:application/pdf}
}

@phdthesis{millward_choice_2003,
  address = {United States -- California},
  type = {Ph.D.},
  title = {‘{{A}} Choice Parcel of Country Born’:  {{African}} -{{Americans}} and the Transition to Freedom in {{Maryland}}, 1770–1840},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {‘{{A}} Choice Parcel of Country Born’},
  abstract = {The Age of Revolution produced a distinct paradox between the ideology of individual liberty and the existence of chattel slavery in the American south. In no instance is this paradox of freedom for some and continued oppression for others more present than in the experiences of slaves in Maryland. Slavery in the United States was not abolished following the American Revolution. Rather, the institution of chattel bondage was re-edified in local and national laws. Revolutionary thought, Christian anti-slavery theology and changing labor demands, however, did make it possible for some slaves to gain freedom through legalized manumission.
Concentrating on the themes of family, freedom and power, this study draws heavily from arguments about the American Revolution, experiences in slavery, the black family, and the simultaneous development of slavery and freedom in Maryland from 1770 to 1840. This work is a detailed treatment of slaves and their evolution into freed persons from the American Revolution into the antebellum period. Often the simultaneous rise of a free black population in the early national period eclipsed detailed discussions of slave life in Maryland. This study places the slave experience at the forefront, acknowledging that, despite the growth of a significant free black community, Maryland remained a slave society.},
  language = {English},
  timestamp = {2016-09-05T00:47:38Z},
  urldate = {2016-09-05},
  school = {University of California, Los Angeles},
  author = {Millward, Jessica},
  year = {2003},
  keywords = {African-Americans,Freedom,Maryland,Social sciences},
  file = {Millward_2003_‘A choice parcel of country born’.pdf:/Users/elipousson/Dropbox/Zotero/storage/MK6FF2ZV/Millward_2003_‘A choice parcel of country born’.pdf:application/pdf}
}

@phdthesis{phelps_mission_1989,
  address = {United States -- District of Columbia},
  type = {Ph.D.},
  title = {The Mission Ecclesiology of {{John R}}. {{Slattery}}: {{A}} Study of an {{African}}-{{American}} Mission of the {{Catholic Church}} in the Nineteenth Century},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The Mission Ecclesiology of {{John R}}. {{Slattery}}},
  abstract = {This dissertation is a critical study of the historical background, the theological motivation, and the mission ecclesiology of John R. Slattery as a Catholic missionary among Black Americans in the socio-political and ecclesial context of the United States during the racially tense atmosphere of the late nineteenth century. The research involved the content analysis of Slattery's published articles, pamphlets and correspondence found in the Leeson-Slattery Papers in the Josephite Fathers' Archives, Baltimore, Maryland and his French autobiography found in the Bibliotheque Nationale, Paris, France. It concludes with some implications for mission and ministry in the Black Community in the twentieth and twenty-first century.
John R. Slattery (1851-1926) was an Irish-American Catholic member of the Mill Hill Fathers of England. Committed to belief in the missionary nature of the church, the "universal brotherhood of all men" and the universal salvific will of God, he labored for the moral "uplift," conversion, and salvation of Black Americans and for their inclusion in the socio-political arena of society which in his view was responsible for the preservation of society. Towards these ends Slattery established several institutions to train missionaries, including Black priests and catechists, whom he saw as essential to any effective and integral mission and ministry in the Black Community.
A natural born leader filled with energy and practical creativity, Slattery was the second American Mill Hill Provincial (1878-1883); founder and first rector of St. Joseph's Seminary in Baltimore, Maryland (1888) and founder of the Epiphany Apostolic College in Baltimore, Maryland (1889) and the St. Joseph's Catechetical College for Negro Catechists in Montgomery, Alabama (1900). James Gibbons, the Cardinal Archbishop of Baltimore appointed Slattery to be the superior of the Society of St. Joseph (1892-1893) which separated from Mill Hill in 1892.
Slattery was influenced and inspired by both Black and Catholic leaders of the nineteenth century among them the delegates of the five Colored Catholic Congresses inaugurated and convened between 1889 and 1894 by Daniel Rudd, a Black Catholic layman. Booker T. Washington, Cardinal Vaughan, Isaac Hecker, Cardinal Lavigerie, Archbishop John Ireland, Walter Elliot, Abbe Houtin and Loisy also played an influential role in shaping Slattery's missionary vision. (Abstract shortened with permission of author.)},
  language = {English},
  timestamp = {2016-09-05T00:50:49Z},
  urldate = {2016-09-05},
  school = {The Catholic University of America},
  author = {Phelps, Jamie Theresa},
  year = {1989},
  keywords = {Philosophy,religion and theology,Social sciences},
  file = {Phelps_1989_The mission ecclesiology of John R.pdf:/Users/elipousson/Dropbox/Zotero/storage/W9W9RPWV/Phelps_1989_The mission ecclesiology of John R.pdf:application/pdf}
}

@phdthesis{hicks_black_1988,
  address = {United States -- Maryland},
  type = {Ph.D.},
  title = {The Black Apprentice in {{Maryland}} Courts Records from 1661 to 1865},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  abstract = {The study examines the apprenticeship system in Maryland during a period of 1661 to 1865 as it pertains to blacks, and as revealed by Maryland court records. Apprenticeship represented one of the earliest forms of education for blacks in Maryland. The study traces the history of apprenticeship from the Greeks and Romans, to the colony of Maryland. Differences of apprenticeship in England and the British West Indies are noted. Cases reviewed of black apprentices before the courts in England before 1800 are described and they show that no laws were established or distinctions made in court decisions based on race or class.
A description of Maryland's apprenticeship system is presented and compared with England's system. While England passed no prohibitive laws against black apprentices, Maryland's apprenticeship law of 1793 did eliminate the requirement to teach black apprentices to read and to write. Despite this prohibitive law, black apprentices continued to have contracts that contained provisions for literacy. In addition to opportunities for literacy, apprenticeship in Maryland afforded some blacks the opportunity for learning and such lucrative crafts as blacksmithing and woodworking.
Employment in these trades as journeymen sometimes led to blacks being able to buy land and retain it long enough to pass it on to the next generation. A description is also given of Maryland's topography and its influence in fostering trades. The industries of tobacco, grain, and ship building provided an array of skilled occupations for which some blacks were apprenticed. However, the availability of these occupations to black apprentices was sometimes influenced by social, political and economic factors.
By the beginning of the nineteenth century, black journeymen were able to form their own unions for some trades, contribute financially toward the building of schools and to sponsor literary societies.},
  language = {English},
  timestamp = {2016-09-05T00:55:47Z},
  urldate = {2016-09-05},
  school = {University of Maryland, College Park},
  author = {Hicks, Helena Sorrell},
  year = {1988},
  keywords = {Education,Social sciences}
}

@phdthesis{dula_prospering_2016,
  address = {United States -- Maryland},
  type = {Ph.D.},
  title = {Prospering {{Because That}}'s {{Its History}}: {{Black Resilience}} and {{Honors Development}} in {{Higher Education}}: {{Morgan State University And}} the {{State}} of {{Maryland}}, 1867-1988},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {Prospering {{Because That}}'s {{Its History}}},
  abstract = {This study explores the origins and development of honors education at a Historically Black College and University (HBCU), Morgan State University, within the context of the Maryland higher education system. During the last decades, public and private institutions have invested in honors experiences for their high-ability students. These programs have become recruitment magnets while also raising institutional academic profiles, justifying additional campus resources. The history of higher education reveals simultaneous narratives such as the tension of post-desegregated Black colleges facing uncertain futures; and the progress of the rise and popularity of collegiate honors programs. Both accounts contribute to tracing seemingly parallel histories in higher education that speaks to the development of honors education at HBCUs. While the extant literature on honors development at Historically White Institutions (HWIs) of higher education has gradually emerged, our understanding of activity at HBCUs is spotty at best. One connection of these two phenomena is the development of honors programs at HBCUs. Using Morgan State University, I examine the role and purpose of honors education at a public HBCU through archival materials and oral histories. Major unexpected findings that constructed this historical narrative beyond its original scope were the impact of the 1935/6 Murray v Pearson, the first higher education desegregation case. Other emerging themes were Morgan’s decades-long efforts to resist state control of its governance, Maryland’s misuse of Morrill Act funds, and the border state’s resistance to desegregation. Also, the broader histories of Black education, racism, and Black citizenship from Dred Scott and Plessy, the 1863 Emancipation Proclamation to Brown, inform this study. As themes are threaded together, Critical Race Theory provides the framework for understanding the emerging themes. In the immediate wake of the post-desegregation era, HBCUs had to address future challenges such as purpose and mission. Competing with HWIs for high-achieving Black students was one of the unanticipated consequences of the Brown decision. Often marginalized from higher education research literature, this study will broaden the research repository of honors education by documenting HBCU contributions despite a challenging landscape.},
  language = {English},
  timestamp = {2016-09-05T00:56:29Z},
  urldate = {2016-09-05},
  school = {University of Maryland, College Park},
  author = {Dula, Traci Leigh Moody},
  year = {2016},
  keywords = {1930-1954,Black higher education,Education,Historically black colleges and universities,Social sciences,State of maryland}
}

