@phdthesis{phillips_negroes_1992,
  address = {United States -- Georgia},
  type = {Ph.D.},
  title = {"{{Negroes}} and Other Slaves": {{The African}}-{{American}} Community of {{Baltimore}}, 1790--1860},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {"{{Negroes}} and Other Slaves"},
  abstract = {The purpose of this study was to examine the African American community of Baltimore, Maryland, during the seventy years prior to the Civil War, with special comparisons to black populations of Charleston and Philadelphia during the same period. This study traces the social transformation of the city's black population from a transient aggregate of migrant freedpeople, fresh from slavery, to a cohesive community, relatively unwracked by class and intra-racial divisions, inclusive of slaves as well as free Negroes, with strong churches, and boasting the establishment of such exclusively black organizations and associations as fraternal orders, literary associations, and debating societies.
While other urban African American communities founded similar social organizations, each suffered from internal class divisions which caused most of their social institutions to be founded by--and reserved for--the community elite. Baltimore's black community did not depend upon wealth, or its wealthy leadership, for strength; its vitality derived from and depended upon the concord of its entire populace--a populace which was overwhelmingly economically poor. Yet this very economic deprivation allowed Baltimore's African American residents to forge a genuine community with which they not only avoided the divisions of other black urban communities, but allowed them to weather the racial tempests of the antebellum years.
Moreover, because of the unique course of manumissions in Maryland after the American Revolution, Baltimore's black residents were overwhelmingly black, rather than mulatto, and as a result the community was far less stratified by skin color than comparable lower South cities such as Charleston and New Orleans. Through this communal organization and group solidarity, Baltimore's black people had riveted together a cohesive urban community, both free and slave, which looked within for identification, yet which also exerted a centrifugal presence of solidarity that reached and affected black persons throughout the upper South. Perhaps more than any other such population, the story of Baltimore's black people exemplifies the nature of a cohesive community perhaps in its truest sense. Through this community strength, Baltimore's black community was able to resist and survive a rising tide of racial antipathy towards free Negroes which swept the nation, but which was particularly acute in Maryland.},
  language = {English},
  timestamp = {2016-08-27T01:51:47Z},
  urldate = {2016-08-27},
  school = {University of Georgia},
  author = {Phillips, Christopher William},
  year = {1992},
  keywords = {Maryland,Social sciences},
  file = {Phillips_1992_Negroes and other slaves.pdf:/Users/elipousson/Dropbox/Zotero/storage/ZS7SKRG9/Phillips_1992_Negroes and other slaves.pdf:application/pdf}
}

@phdthesis{kelly_persistent_2009,
  address = {United States -- Maryland},
  type = {M.A.},
  title = {The Persistent Presence of a Successful Failure: {{The Maryland State Colonization Society}} and the Importance of Its Existence},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The Persistent Presence of a Successful Failure},
  abstract = {In 1832, the Maryland State Colonization Society officially seceded from the American Colonization Society. With this declaration of independence, the Maryland State Colonization Society embarked on a thirty year journey during which it failed miserably in its very purpose. The Maryland State Colonization Society failed to convince free African Americans to immigrate to Africa in order to pursue a life free of oppression and racism. Nevertheless, the Maryland State Colonization Society sustained an official and financial relationship with the Maryland General Assembly during this period. The thesis explores the underlying motivations and controversies which allowed the Maryland General Assembly to look to the Maryland State Colonization Society as the answer to its own sectional “crises.” Through the rhetoric and arguments published in the Maryland Colonization Journal, the Maryland State Colonization Society became a voice for moderation and compromise amongst the divisive and sectional viewpoints of the North and South. Providing a program which slaveholders and anti-slavery proponents could suit to their own beliefs, the white population of Maryland could believe that they had answered the “problem” of slavery and a growing free African American population. Through the Maryland State Colonization Society's program, the population of Maryland made a deliberate and prolonged evaluation of slavery's present and future role in the state. Consequently, this allowed the legislatures and leaders of Maryland to make the pragmatic decision to remain in the Union during the Civil War.},
  language = {English},
  timestamp = {2016-08-27T01:52:47Z},
  urldate = {2016-08-27},
  school = {University of Maryland, Baltimore County},
  author = {Kelly, Kevin},
  year = {2009},
  keywords = {1831-1870,Colonization,Maryland,Maryland Colonization Journal,Sectionalism,Social sciences},
  file = {Kelly_2009_The persistent presence of a successful failure.pdf:/Users/elipousson/Dropbox/Zotero/storage/FH4699UD/Kelly_2009_The persistent presence of a successful failure.pdf:application/pdf}
}

@phdthesis{morrow_oblate_1996,
  address = {United States -- Georgia},
  type = {Ph.D.},
  title = {The {{Oblate Sisters}} of {{Providence}}: {{Issues}} of Black and Female Agency in Their Antebellum Experience, 1828-1860},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  shorttitle = {The {{Oblate Sisters}} of {{Providence}}},
  abstract = {The Oblate Sisters of Providence, co-founded by Sulpician priest James Joubert and mulatto Caribbean emigre Elizabeth Clarisse Lange in Baltimore in 1828, formed the first permanent community of African American Roman Catholic women religious in the world. During the 1830s the Oblate Sisters forged the patterns of their communal life, conducted a school for black girls, delineated their complex relationship with the black lay community, and sought acceptance as a legitimate constituency within both the antebellum Roman Catholic Church and southern society.
During the 1840s the Oblate community confronted a series of crises: the death of co-founder James Joubert, the first departures and dismissal of professed members, and apparent desertion by church authorities. For four years the Oblate Sisters persevered in their observance of religious life and the conduct of their school without benefit of official clerical sponsorship. Oblate steadfastness attracted the support of the Redemptorist priests who assumed Oblate spiritual direction in 1847.
During the 1850s the Oblate Sisters expanded their teaching mission, refined their relationship with the black lay community, and reformulated their school curriculum and prospectus to reflect their status as a tested and mature religious community.
The Oblate Sisters both challenged and responded to the attitudes of the Church and the South about race, gender, religion, and ethnicity. Embracing the religious state proved both liberating and empowering to individuals debased by the worldly social order. In renouncing the world to consecrate themselves to God, the Oblate Sisters achieved symbolic release from socially ascribed derogation based on their identities as black women. As Oblate Sisters of Providence, women of color in the antebellum South utilized their piety and spiritual fervor to defy their socially ascribed inferior status and to exercise agency in service to others.
The Oblate Sisters inculcated into their communal consciousness positive senses of themselves as black women and as committed religious. These empowering self-images prevailed for the Oblate membership over both the ambivalence toward them as black women religious plaguing the institutional Church and the disdain toward them as both black and women religious demonstrated by white southern society.},
  language = {English},
  timestamp = {2016-08-27T01:53:21Z},
  urldate = {2016-08-27},
  school = {University of Georgia},
  author = {Morrow, Diane Batts},
  year = {1996},
  keywords = {African-Americans,Catholicism,Elizabeth Clarisse,James,Joubert,Lange,Language,literature and linguistics,Maryland,Philosophy,religion and theology,Social sciences,women religious},
  file = {Morrow_1996_The Oblate Sisters of Providence.pdf:/Users/elipousson/Dropbox/Zotero/storage/2MBM29AP/Morrow_1996_The Oblate Sisters of Providence.pdf:application/pdf}
}

